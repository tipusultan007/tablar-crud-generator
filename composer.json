{
    "name": "takielias/tablar-crud-generator",
    "description": "Laravel Tablar Crud Generator based on https://github.com/takielias/tablar",
    "license": "MIT",
    "authors": [
        {
            "name": "Taki Elias",
            "email": "taki.elias@gmail.com"
        }
    ],
    "keywords": [
        "laravel",
        "Tablar",
        "crud",
        "crud generator",
        "laravel crud generator",
        "laravel package"
    ],
    "require": {
        "laravel/framework": "^10.0|^11.0",
        "laravelcollective/html": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Tablar\\CrudGenerator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tablar\\CrudGenerator\\CrudServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
